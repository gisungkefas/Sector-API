{
    "version": 2,
    "builds": [
      {"src": "./app.js", "use": "@vercel/node"}
    ],
    "routes": [
      {
        "src": "/api/sectors",
        "dest": "/api/sectors"
      },
      {
        "src": "/api/uploads",
        "dest": "/api/uploads"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  