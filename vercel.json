{
    "version": 2,
    "builds": [
      {"src": "./app.js", "use": "@vercel/node"}
    ],
    "routes": [
      {
        "src": "/api/sectors",
        "dest": "/api/sectors"
      },
      {
        "src": "/api/uploads",
        "dest": "/api/uploads"
      },
      {
        "src": "/favicon.ico",
        "status": 204,
        "headers": {
          "cache-control": "public, max-age=0, must-revalidate"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  